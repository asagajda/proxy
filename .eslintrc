{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true,
  },

  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-extra-strict": 0,
    "strict": 0,
    "curly": [0],
    "semi": [2, "never"]
  }
}